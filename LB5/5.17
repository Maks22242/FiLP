
open System

//Обход делителей числа
let rec Divid_rec1 (a:int,init:int,beg:int,func: int->bool):int = 
    match beg with
    | beg when beg>a/2 -> init
    | beg when ((a%beg=0)&&(func beg)) ->
          printfn $"{init}) {beg}"
          Divid_rec1(a,init+1 , beg+1, func)              
    | beg -> Divid_rec1(a,init,beg+1,func)

let UnFun1 (a:int,init:int,func: int->bool) = 
    printfn"Делители удовлетворяющие условию 'a<10':"
    Divid_rec1(a,init,1,func)

//Обход взаимопростых чисел
let rec Nod a b =
    match a with 
    | _ when a%b =0 -> b
    | _-> Nod b (a%b)


let rec Divid_rec2 (a:int,init:int,beg:int,func: int->bool):int = 
    match beg with
    | beg when beg=a-> init
    | beg when ((Nod a beg = 1)&&(func beg)) -> 
          printfn $"{init}) {beg}"
          Divid_rec2(a, init+1 , beg+1, func)

    | beg -> Divid_rec2(a,init,beg+1,func)

let UnFun2 (a:int,init:int,func: int->bool) = 
    printfn"Взаимно промтые кампаненты удовлетворяющие условию 'a<10':"
    Divid_rec2(a,init,1,func)

